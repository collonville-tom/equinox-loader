<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ConfigurationLibrary xmlns="http://collonville.thomas.fr/tc-osgi-bundle-utils/config-file-schema">

	<configFiles>
		<defaultValue>/opt/equinox-loader/bundles</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>bundleDirectory</fieldName>
		<className>org.tc.osgi.bundle.manager.conf.ManagerPropertyFile</className>
	</configFiles>
	<configFiles>
		<defaultValue>file://localhost://</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>bundleLocalBase</fieldName>
		<className>org.tc.osgi.bundle.manager.conf.ManagerPropertyFile</className>
	</configFiles>
	<configFiles>
		<defaultValue>${groovy.version}</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>groovyDependencyBundleVersion</fieldName>
		<className>org.tc.osgi.bundle.manager.module.activator.ManagerActivator</className>
	</configFiles>
	<configFiles>
		<defaultValue>tc-osgi-bundle-groovy</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>groovyDependencyBundleName</fieldName>
		<className>org.tc.osgi.bundle.manager.module.activator.ManagerActivator</className>
	</configFiles>
	<configFiles>
		<defaultValue>7001</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>jmxPort</fieldName>
		<className>org.tc.osgi.bundle.manager.jmx.JMXInterface</className>
	</configFiles>
</ConfigurationLibrary>