<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ConfigurationLibrary
	xmlns="http://collonville.thomas.fr/tc-osgi-bundle-utils/config-file-schema">
	<configFiles>
		<defaultValue>/opt/equinox-loader/bundles</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>bundleDirectory</fieldName>
		<className>org.tc.osgi.equinox.loader.conf.EquinoxPropertyFile</className>
	</configFiles>
	<configFiles>
		<defaultValue>file://localhost://</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>bundleLocalBase</fieldName>
		<className>org.tc.osgi.equinox.loader.conf.EquinoxPropertyFile</className>
	</configFiles>
	<configFiles>
		<defaultValue>-console</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>mode</fieldName>
		<className>org.tc.osgi.equinox.loader.starter.EquinoxStarter</className>
	</configFiles>
    <configFiles>
        <defaultValue>-debug</defaultValue>
        <valueType>java.lang.String</valueType>
        <fieldName>debugLevel</fieldName>
        <className>org.tc.osgi.equinox.loader.starter.EquinoxStarter</className>
    </configFiles>
    <configFiles>
        <defaultValue>/opt/equinox-loader/bundles/configuration</defaultValue>
        <valueType>java.lang.String</valueType>
        <fieldName>configuration</fieldName>
        <className>org.tc.osgi.equinox.loader.starter.EquinoxStarter</className>
    </configFiles>
	<configFiles>
		<defaultValue>tc-osgi-bundle-manager</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>managerDependencyBundleName</fieldName>
		<className>org.tc.osgi.equinox.loader.cmd.context.StartDefaultBundleCmd</className>
	</configFiles>
	<configFiles>
		<defaultValue>tc-osgi-bundle-utils</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>utilsDependencyBundleName</fieldName>
		<className>org.tc.osgi.equinox.loader.cmd.context.StartDefaultBundleCmd</className>
	</configFiles>
    <configFiles>
        <defaultValue>tc-osgi-bundle-utils</defaultValue>
        <valueType>java.lang.String</valueType>
        <fieldName>utilsDependencyBundleName</fieldName>
        <className>org.tc.osgi.equinox.loader.cmd.context.FilterValidBundleCmd</className>
    </configFiles>
	<configFiles>
		<defaultValue>14689</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>rmiPort</fieldName>
		<className>org.tc.osgi.equinox.loader.rmi.EquinoxLoaderRMIServer</className>
	</configFiles>
	<configFiles>
		<defaultValue>localhost</defaultValue>
		<valueType>java.lang.String</valueType>
		<fieldName>rmiAddr</fieldName>
		<className>org.tc.osgi.equinox.loader.rmi.EquinoxLoaderRMIServer</className>
	</configFiles>
</ConfigurationLibrary>